<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sefapplication.LoggedInSceneController">
   <children>
      <Pane prefHeight="200.0" prefWidth="480.0" style="-fx-background-color: #0675ba;" />
      <Pane layoutY="200.0" prefHeight="520.0" prefWidth="480.0" style="-fx-background-color: #daeef3;" />
      <Button fx:id="logoutButton" layoutX="100.0" layoutY="20.0" mnemonicParsing="false" onAction="#switchToMainScene" prefHeight="60.0" prefWidth="100.0" style="-fx-background-color: #025a99;" text="Logout" textFill="WHITE">
         <effect>
            <Glow level="0.15" />
         </effect>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="cartButton" layoutX="280.0" layoutY="20.0" mnemonicParsing="false" onAction="#switchToCartScene" prefHeight="60.0" prefWidth="100.0" style="-fx-background-color: #025a99;" text="Cart" textFill="WHITE">
         <effect>
            <Glow level="0.15" />
         </effect>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="searchBar" layoutX="101.0" layoutY="110.0" prefHeight="35.0" prefWidth="320.0" promptText="Search..." style="-fx-background-color: #6699ff;">
         <effect>
            <Glow level="0.15" />
         </effect>
      </TextField>
      <Pane layoutX="65.0" layoutY="110.0" prefHeight="35.0" prefWidth="35.0" style="-fx-background-color: #6699ff;">
         <effect>
            <Glow level="0.2" />
         </effect>
         <children>
            <ImageView fx:id="searchButton" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../Images/searchButton.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Button layoutX="65.0" layoutY="155.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="85.0" style="-fx-background-color: #6699ff;" text="All" textFill="WHITE">
         <effect>
            <Glow level="0.15" />
         </effect>
      </Button>
      <Button layoutX="160.0" layoutY="155.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #6699ff;" text="Beds" textFill="WHITE">
         <effect>
            <Glow level="0.15" />
         </effect>
      </Button>
      <Button layoutX="250.0" layoutY="155.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #6699ff;" text="Chairs" textFill="WHITE">
         <effect>
            <Glow level="0.15" />
         </effect>
      </Button>
      <Button layoutX="340.0" layoutY="155.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #6699ff;" text="Tables" textFill="WHITE">
         <effect>
            <Glow level="0.15" />
         </effect>
      </Button>
   </children>
</AnchorPane>
